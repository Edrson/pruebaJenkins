pipeline {
  agent any
    
 
    
  stages {
        
    stage('Git') {
      steps {
        git 'https://github.com/Edrson/pruebaJenkins'
      }
    }
     
    stage('Build') {
      steps {
        bat 'npm install'
        
      }
    }  
    
            
    stage('Test') {
      steps {
        bat 'npm run test'
      }
    }
  }
}
